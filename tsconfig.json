{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "preserve",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "screens/*": [
        "src/screens/*"
      ],
      "utils/*": [
        "src/utils/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "stores/*": [
        "src/stores/*"
      ],
    }
  }
}